<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="info">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout>
        <Pattern>%d %p %c{1.} [%t] %m %ex%n</Pattern>
      </PatternLayout>
    </Console >
  </Appenders>
  <Loggers>
    <Root level="info" includeLocation="false">
      <appender-ref ref="Console" level="info"/>
    </Root>
  </Loggers>
</Configuration>